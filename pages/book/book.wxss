.header{
  position: fixed;
  z-index: 99;
  background: white;
  height: 100rpx;
  width: 100%;
  border-top:1px solid #f5f5f5;
  border-bottom:1px solid #f5f5f5;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  box-shadow:0 0 3px 0 #e3e3e3;
}
.box{
  height: 68rpx;
  width: 700rpx;
  background: #f5f5f5;
  border-radius: 50px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.box .placeholder{
  font-size: 28rpx;
  text-align: center;
  margin-left:10rpx
}
.sub-container{
  background: #f5f5f5;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 100rpx;
}

.sub-container .quality {
  width:106rpx;
  height:34rpx;
  margin-top:40rpx;
}
.sub-container .book-list{
  margin-top:10rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  padding: 0 90rpx 0 90rpx;
  justify-content: space-between;
}